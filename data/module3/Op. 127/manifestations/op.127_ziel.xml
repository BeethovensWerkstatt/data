                                    </staffGrp>
                                </scoreDef>
                                <measure>
                                    <staff n="1">
                                        <layer n="1">
                                            <note xml:id="note5" pname="b" oct="5" dur="2"/>
                                            <note xml:id="note6" pname="f" oct="6" dur="4"/>
                                            <note xml:id="note7" pname="d" oct="6" dur="4"/>
                                        </layer>
                                    </staff>
                                    <slur staff="1" startid="#note5" endid="#note7" curvedir="below"/>
                                </measure>
                            </section>
                        </notatedMusic>
                     
                        <lb/>heißen: 
                        <notatedMusic>
                            <section xmlns="http://www.music-encoding.org/ns/mei">
