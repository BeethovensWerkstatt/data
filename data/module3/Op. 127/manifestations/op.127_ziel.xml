                    <lb/>
                    
                    <seg type="#bw_monitum_context">
                        <!-- <seg> statt mei:annot/@class='bw_monitum_context' -->
                        
                        <lb/>II) In dem <w facs="svgShapesProWort">Quartett</w> kommt im 3<hi rend="superscript">ten</hi> Stück, nach dem Presto 3/4-Takt, ein à tempo 3/4, in diesem ist im 17<hi rend="superscript">ten</hi> Takte der 2<hi rend="superscript">ten</hi> Violin, statt 
                       
                        <notatedMusic >
                            <section xmlns="http://www.music-encoding.org/ns/mei">
                                <scoreDef key.sig="3f" meter.count="2" meter.unit="4">
                                    <staffGrp>
                                        <staffDef n="1" lines="5" clef.shape="G" clef.line="2"/>
                                    </staffGrp>
                                </scoreDef>
                                <measure>
                                    <staff n="1">
                                        <layer n="1">
                                            <note xml:id="note1" pname="b" oct="4" dur="2"/>
                                            <note xml:id="note2" pname="f" oct="5" dur="4"/>
                                        </layer>
                                    </staff>
                                    <trill startid="#note2"/>
                                    <slur staff="1" startid="#note1" endid="#note2" curvedir="above"/>
                                </measure>
                            </section>
                        </notatedMusic>
                        
                        zu setzen:
                        
                        <notatedMusic>
                            <section xmlns="http://www.music-encoding.org/ns/mei">
                                <scoreDef key.sig="3f" meter.count="2" meter.unit="4">
                                    <staffGrp>
