                                            <note xml:id="note4" pname="f" oct="5" dur="4"/>
                                        </layer>
                                    </staff>
                                    <trill startid="#note4"/>
                                    <slur staff="1" startid="#note3" endid="#note4" curvedir="above"/>
                                </measure>
                            </section>
                        </notatedMusic>
                        
                        . (Partitur, Seite 30.)
                    </seg>
                    
                    <seg type="#bw_monitum_context">
                       <lb/>
